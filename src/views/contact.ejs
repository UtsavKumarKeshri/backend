
<div class="fdbk-form-ContentBox">
  
    <div class="fdbk-formControl">
    <h1 id="fdbk-form-heading-name">Feedback Form</h1>
   <h2>Login</h2>
    <form action="/contact" method="post" class="fdbk-form-group">
      
      <div>
      <label for="Name">Name</label>
      <input 
      type="text" 
      name="email" id="fdbk-form-user-name"
      placeholder="Your Name Here"/>
    </div>
      <div>
      <label for="Name">Email</label>
      <input 
      type="text" 
      name="email" id="fdbk-form-user-email"
      placeholder="Your email Here"/>
    </div>
    
    <div>
      <label for="Message">Message</label>
      <textarea name="Message" 
      id="user-message" 
      cols="20" 
      rows="5"
      placeholder="Type your message..."></textarea>
      
    </div>
    <button id="fdbk-form-Submit">Submit</button>
    </form>
  </div>
